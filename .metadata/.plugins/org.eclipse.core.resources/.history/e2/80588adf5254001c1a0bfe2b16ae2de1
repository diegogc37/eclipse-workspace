	public String toString() {
		String retorno="";
		for(int i = 0; i<otroVector.length;i++ ) {
			retorno+=otroVector[i]+" ";
		}
		return retorno;
	}
	
	public void imprimir(int[] array) {
		System.out.println("{");
		for(int i = 0; i<array.length;i++) {
			System.out.println(array[i]+" ");
		}
		System.out.println("}");
	}
	
