	public static void main(String[] args) {
		OperacionesArrays operaciones=new OperacionesArrays();

	}

	
	public String toString() {
		String retorno="";
		for(int i = 0; i<otroVector.length;i++ ) {
			retorno+=otroVector[i]+" ";
		}
		return retorno;
	}
	
	public void imprimir(int[] array) {
		System.out.print("{ ");
		for(int i = 0; i<array.length;i++) {
			System.out.print(array[i]+" ");
		}
		System.out.println("}");
	}
	
	public void inicializar(int[] array, int valorMax) {
		Random r = new Random();
		for(int i = 0; i<array.length;i++) {
			array[i]=Math.abs(r.nextInt())%valorMax+1;
		}
	}
	
	public int[] ordenarArray(int[] array) {
		int[] arrayOrdenado=new int[array.length];
		return arrayOrdenado;
	}
	
}
